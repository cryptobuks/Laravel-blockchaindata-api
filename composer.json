{
    "name": "waygood/blockchaindata",
    "description": "This is a package designed to integrate with laravel 5.0+",
    "keywords": ["php", "bitcoin", "blockchain", "data", "cryptocurrency", "Laravel", "open source", "laravel 5"],
    "license": "MIT",
    "authors": [
    	{
    		"name": "Matthew Waygood",
    		"email": "matthew@waygood.co.uk"
    	}
    ],
    "minimum-stability": "stable",
    "require": {
    "php": "^5.4.0|^7.0",
    "guzzlehttp/guzzle": "5.*|6.*"
   },
   "require-dev": {
        "phpunit/phpunit" : "4.*"
    },
    "autoload": {
        "psr-4": {
          "Waygood\\BlockchainData\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Waygood\\BlockchainData\\BlockchainDataServiceProvider"
            ],
            "aliases": {
                "BlockchainData": "Waygood\\BlockchainData\\Facades\\BlockchainData"
            }
        }
    }
}